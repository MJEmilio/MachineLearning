   ],
   "source": [
    "evals,evecs = np.linalg.eig(C)\n",
    "indices = np.argsort(evals)\n",
    "indices = indices[::-1]\n",
    "evecs = evecs[:,indices]\n",
    "evals = evals[indices]\n",
    "\n",
    "new_data = np.dot(evecs.T, data.T) \n",
    "\n",
    "for i, c, label in zip(target_ids, 'rgb', iris.target_names):\n",
    "    plt.scatter(new_data[ 0,y == i], new_data[1,y == i],\n",
    "               c=c, label=label, marker='.')\n",
    "plt.legend()"
   ]
